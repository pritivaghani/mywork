package impractical;

public class Primenumbers {

	public static void main(String[] args) {
		int n = 100, i = 2, fact = 0;
		for(i=2;i<=n;i++)
		{
			if(fact==0)    {
				fact = 1;
			}
		
			if(fact!=0)   {
				System.out.println("number is prime");
			}
				else
				{
                System.out.println("number is not prime");				
			}
			}
		}
	}



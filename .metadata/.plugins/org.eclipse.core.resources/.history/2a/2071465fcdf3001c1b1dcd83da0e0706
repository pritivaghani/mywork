package impractical;

public class Primenumbers {

	public static void main(String[] args) {
		
	    
	    int a = 100;
		 int flag = 0;
		for(int i=3;i<=a;i++)
		{
		  if(a%i==0)
		  {
			flag = 1;
			break;
		   }
		}
		if (flag==0)
		{
			System.out.println('i'+ "prime number");
		}
			else
			{
				System.out.println('i'+ " not prime number");
		}
		
		}
	    }
	
}    

	



